{
  "name": "egg-bullboard-plugin",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "eggjs bullboard adapter plugin for bullmq",
  "eggPlugin": {
    "name": "bullboard",
    "exports": {
      "import": "./dist/esm",
      "require": "./dist/commonjs",
      "typescript": "./src"
    },
    "dependencies": [
      "static",
      "ejs"
    ]
  },
  "keywords": [
    "egg",
    "egg-plugin",
    "eggPlugin",
    "egg-bullboard",
    "bullboard/api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorYao/egg-bullboard-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/TylorYao/egg-bullboard-plugin/issues"
  },
  "homepage": "https://github.com/TylorYao/egg-bullboard-plugin#readme",
  "author": "tylor.yao",
  "license": "MIT",
  "engines": {
    "node": ">= 18.19.0"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@eggjs/core": "^6.2.13"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@eggjs/bin": "7",
    "@eggjs/mock": "6",
    "@eggjs/tsconfig": "1",
    "@types/mocha": "10",
    "@types/node": "22",
    "egg": "4",
    "eslint": "8",
    "eslint-config-egg": "14",
    "rimraf": "6",
    "tshy": "3",
    "tshy-after": "1",
    "typescript": "5"
  },
  "scripts": {
    "lint": "eslint --cache src test --ext .ts",
    "pretest": "npm run clean && npm run lint -- --fix",
    "test": "egg-bin test",
    "preci": "npm run clean &&  npm run lint",
    "ci": "egg-bin cov",
    "postci": "npm run prepublishOnly && npm run clean",
    "clean": "rimraf dist",
    "prepublishOnly": "tshy && tshy-after && attw --pack"
  },
  "type": "module",
  "tshy": {
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json"
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/commonjs/index.d.ts",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js"
}