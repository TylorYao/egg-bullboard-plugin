{
  "name": "egg-bullboard-plugin",
  "version": "3.1.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "eggjs bullboard adapter plugin for bullmq",
  "peerDependencies": {
    "bullmq": "^5.0.0"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.2"
  },
  "devDependencies": {
    "bullmq": "^5.0.0",
    "@arethetypeswrong/cli": "^0.17.3",
    "@eggjs/tsconfig": "^1.3.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.1.0",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "egg": "^3.15.0",
    "egg-bin": "^6.1.2",
    "egg-ci": "^2.1.0",
    "egg-mock": "^5.10.6",
    "egg-view-ejs": "^3.0.0",
    "eslint": "^8.16.0",
    "eslint-config-egg": "^12.1.0",
    "semantic-release": "^19.0.5",
    "tsd": "^0.26.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "tsd": "tsd",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov && npm run prepublishOnly",
    "build": "tsc -p ./tsconfig.json",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "config/*.js",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "agent.js",
    "app.js",
    "index.d.ts"
  ],
  "eggPlugin": {
    "name": "bullboard",
    "dependencies": [
      "static",
      "view"
    ]
  },
  "keywords": [
    "egg",
    "egg-plugin",
    "eggPlugin",
    "egg-bullboard",
    "bullboard/api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorYao/egg-bullboard-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/TylorYao/egg-bullboard-plugin/issues"
  },
  "homepage": "https://github.com/TylorYao/egg-bullboard-plugin#readme",
  "author": "tylor.yao",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}